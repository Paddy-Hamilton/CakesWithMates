{
  "name": "cakes-with-mates--frontend",
  "author": "Paddy Hamilton",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@apollo/react-ssr": "^3.0.1",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.2.12",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "apollo-link-state": "^0.4.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "graphql": "^14.4.2",
    "graphql-anywhere": "^4.1.10",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-unfetch": "^2.0.0",
    "jss": "latest",
    "moment": "^2.22.1",
    "next": "9.0.3",
    "next-apollo": "^3.0.3",
    "next-with-apollo": "^4.2.0",
    "prisma-binding": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-adopt": "^0.6.0",
    "react-apollo": "^3.0.1",
    "react-dom": "^16.9.0",
    "react-infinite-scroller": "^1.1.4",
    "react-jss": "latest",
    "truncate": "^2.0.1",
    "uuid": "^3.2.1",
    "waait": "^1.0.2",
    "next-plugin-graphql": "^0.0.2"
  },
  "scripts": {
    "dev": "rimraf node_modules/.cache && node server.js ",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "jest --watch",
    "test:updateSnap": "jest --updateSnapshot",
    "deploy": "now --public --regions bru -e NODE_ENV=production "
  },
  "devDependencies": {
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-inline-import-graphql-ast": "^2.3.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "graphql-import": "^0.5.2",
    "jest": "^23.1.0",
    "jest-transform-graphql": "^2.1.0",
    "next-plugin-graphql": "^0.0.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ]
      }
    }
  }
}
